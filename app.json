{
  "expo": {
    "extra": {
      "proxy": "http://192.168.0.103:3000/compare_faces"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ProtecTa to access your camera."
        }
      ]
    ],
    "name": "protecta",
    "slug": "expo-template-tabs",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Homescreen.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/wlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wlogo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/text.png"
    }
  }
}
